server {
    listen 80;
    location /static/ {
        alias /staticfiles/;
        index parser.html;
    }
    location /admin/ {
        proxy_set_header Host $http_host;
        proxy_pass http://network_parser_container:8001/admin/;
    }
    location / {
        proxy_set_header Host $http_host;
        proxy_pass http://network_parser_container:8001/;
    }
}